{
  "name": "campus-lost-found",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev --name init",
    "seed": "node prisma/seed.mjs",
    "db:push:test": "cross-env NODE_ENV=test DATABASE_URL=file:./prisma/test.db npx prisma db push --skip-generate",
    "test:integration": "cross-env NODE_ENV=test vitest run --reporter=verbose",
    "db:gen:test": "cross-env NODE_ENV=test DATABASE_URL=file:./prisma/test.db npx prisma generate",
    "db:push": "prisma db push --skip-generate",
    "seed:dev": "tsx scripts/seed-dev.ts",
    "coverage:txt": "mkdir -p coverage && npm run coverage | tee coverage/coverage.txt"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "@types/bcryptjs": "^2.4.6",
    "axios": "^1.13.2",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "next": "16.0.1",
    "prisma": "^6.18.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "swagger-ui-react": "^5.30.2",
    "swr": "^2.3.6",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.7",
    "bcryptjs": "^3.0.3",
    "c8": "^10.1.3",
    "cross-env": "^10.1.0",
    "cross-fetch": "^4.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "jest": "^29.7.0",
    "next-test-api-route-handler": "^4.0.16",
    "postcss": "^8.5.6",
    "supertest": "^7.1.4",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.7"
  }
}
